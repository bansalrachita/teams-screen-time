{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.7/MicrosoftTeams.schema.json",
  "manifestVersion": "1.7",
  "version": "{version}",
  "id": "{appid}",
  "packageName": "com.app.teams.screentime",
  "developer": {
    "name": "Rachita Bansal",
    "websiteUrl": "{baseUrl0}",
    "privacyUrl": "{baseUrl0}/privacy",
    "termsOfUseUrl": "{baseUrl0}/termsofuse"
  },
  "icons": {
    "color": "color.png",
    "outline": "outline.png"
  },
  "name": {
    "short": "{appname}",
    "full": "{fullappname}"
  },
  "description": {
    "short": "{appname}",
    "full": "Full description of {appname} for Microsoft Teams."
  },
  "accentColor": "#FFFFFF",
  "bots": [],
  "composeExtensions": [],
  "configurableTabs": [],
  "staticTabs": [
    {
      "entityId": "index",
      "name": "Activity Overview",
      "contentUrl": "{baseUrl0}/tab",
      "scopes": ["personal"]
    }
  ],
  "permissions": ["identity", "messageTeamMembers"],
  "validDomains": ["*.ngrok.io", "*.microsoft.com"],
  "webApplicationInfo": {
    "id": "{appclientid}",
    "resource": "api://{baseUrl0}/{appclientid}"
  }
}
